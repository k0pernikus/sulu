<div class="grid">
    <div class="grid-row">
        <div class="grid-col-12">
            <h1>
                {% block title %}{% endblock %}
            </h1>
        </div>
        <div class="grid-col-12">
            {% block options %}
                <div class="pull-right">
                    <div id="options-dropdown">

                    </div>
                </div>
            {% endblock %}
        </div>
    </div>
</div>

{% block list %}{% endblock %}

{% block content %}{% endblock %}

{% block dialog %}<div id="dialog"></div>{% endblock %}
